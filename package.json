{
  "name": "@turnipxenon/pineapple",
  "description": "personal package for base styling for other personal projects",
  "version": "5.0.0-alpha.10",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "generate": "npx vite-node src/routes/blogs/blog_list_data_generator.ts && npx vite-node src/routes/toyay/toya_files_generator.ts && npm run generate-yarn",
    "generate-yarn": "npx vite-node src/lib/scripts/pineapple_fiber/PineappleWeaver.ts",
    "custom-check": "npx vite-node src/lib/scripts/util/ManualCheckRun.ts",
    "preview": "pnpm run build && wrangler dev",
    "prepublishOnly": "pnpm package",
    "prepare": "husky",
    "deploy": "wrangler deploy",
    "cf-typegen": "wrangler types ./src/worker-configuration.d.ts"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/compat": "^1.4.1",
    "@eslint/js": "^9.39.2",
    "@sveltejs/adapter-cloudflare": "^7.2.6",
    "@sveltejs/package": "^2.5.7",
    "@types/htmlparser2": "^3.10.7",
    "@types/mdast": "^4.0.4",
    "@types/node": "^20.19.30",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.14.0",
    "globals": "^16.5.0",
    "highlight.js": "^11.11.1",
    "htmlparser2": "^9.1.0",
    "husky": "^9.1.7",
    "madge": "^8.0.0",
    "mdast": "^3.0.0",
    "mdast-util-from-markdown": "^1.3.1",
    "node-html-parser": "^6.1.13",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "prisma": "^5.22.0",
    "publint": "^0.2.12",
    "sass": "^1.97.3",
    "string-width": "^7.2.0",
    "svelte": "^5.49.1",
    "svelte-check": "^4.3.6",
    "svelte2tsx": "^0.7.47",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "vite": "^7.3.1",
    "wrangler": "4.59.1",
    "@sveltejs/kit": "2.49.5",
    "@inlang/paraglide-js": "~2.4.0",
    "@prisma/client": "^5.22.0"
  },
  "dependencies": {
    "melt": "^0.44.0",
    "mode-watcher": "^0.5.1",
    "shiki": "^3.22.0",
    "shiki-transformer-copy-button": "0.0.3",
    "svelte-modals": "^2.0.1"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/ui/components/index.d.ts",
      "svelte": "./dist/ui/components/index.js",
      "import": "./dist/ui/components/index.js",
      "default": "./dist/ui/components/index.js"
    },
    "./elements": {
      "types": "./dist/ui/elements/index.d.ts",
      "svelte": "./dist/ui/elements/index.js",
      "import": "./dist/ui/elements/index.js",
      "default": "./dist/ui/elements/index.js"
    },
    "./modules": {
      "types": "./dist/ui/modules/index.d.ts",
      "svelte": "./dist/ui/modules/index.js",
      "import": "./dist/ui/modules/index.js",
      "default": "./dist/ui/modules/index.js"
    },
    "./templates": {
      "types": "./dist/ui/templates/index.d.ts",
      "svelte": "./dist/ui/templates/index.js",
      "import": "./dist/ui/templates/index.js",
      "default": "./dist/ui/templates/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*",
    "src/lib/styles/app.css",
    "src/lib/theme.css"
  ],
  "engines": {
    "node": ">=22"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TurnipXenon/pineapple.git"
  },
  "author": "turnipxenon <turnipxenon@gmail.com>",
  "license": "MIT",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
